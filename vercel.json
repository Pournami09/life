{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/life-in-weeks.yml",
      "dest": "/life-in-weeks.yml",
      "headers": {
        "Content-Type": "text/yaml"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}






